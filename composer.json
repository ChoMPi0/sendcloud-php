{
    "name": "chompi0/sendcloud-php",
    "description": "A PHP library to simplify integration with the Sendcloud API",
    "version": "0.1.0",
    "keywords": ["sendcloud", "api", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nedyalko Chakandrakov",
            "homepage": "https://www.linkedin.com/in/nedyalko-chakandrakov-31a6921a6/"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.3",
        "adhocore/jwt": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Sendcloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sendcloud\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
